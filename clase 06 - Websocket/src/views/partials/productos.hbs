<div class="container">
<h2>Productos</h2>
<table class="table table-dark">
      <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody id="tableBody">

    {{#each productos}}
      <tr>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td><img src="{{this.thumbnail}}" alt="{{this.title}}"></td>
      </tr>
    {{/each}}

  </tbody>
  </table>

</div>

